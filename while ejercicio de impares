#include "stdafx.h"
#include <iostream>
#include "conio.h"
#include "math.h"

using namespace std;

void main()

{
   int suma,N,i,cont;
   cout<<"ingrese N:";
   cin>> N;
   suma=0;
   i=1;
   cont=1;
   while (i<=N)
   {     
     suma=suma+cont;
	 cont=cont+2;
     i=i+1;
     
   }
   cout<<"mostrar suma:";
   cout<<suma;

getch();

}
